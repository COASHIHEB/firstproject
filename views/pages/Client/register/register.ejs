<!DOCTYPE html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>Star Admin Free Bootstrap Admin Dashboard Template</title>
  <!-- plugins:css -->
  <link rel="stylesheet" href="../../vendors/iconfonts/mdi/css/materialdesignicons.min.css">
  <link rel="stylesheet" href="../../vendors/css/vendor.bundle.base.css">
  <link rel="stylesheet" href="../../vendors/css/vendor.bundle.addons.css">
  <!-- endinject -->
  <!-- plugin css for this page -->
  <!-- End plugin css for this page -->
  <!-- inject:css -->
  <link rel="stylesheet" href="../../css/style.css">
  <!-- endinject -->
  <link rel="shortcut icon" href="../../images/favicon.png" />
</head>

<body>

  <div class="container-scroller">
    <div class="container-fluid page-body-wrapper full-page-wrapper auth-page">
      <div class="content-wrapper d-flex align-items-center auth register-bg-1 theme-one">
        <div class="row w-100">
          <div class="col-lg-4 mx-auto">
            <div class="auto-form-wrapper">
                <h2 id="insVal" class="text-center mb-4">Inscription</h2>
              <div id="remove">
                <div class="form-group">
                  <div class="input-group">
                    <input id = "nom" type="text" class="form-control" placeholder="Nom" pattern="[A-Za-z]+" required>
                    <div class="input-group-append">
                      <span class="input-group-text"  id="nomValide">
                        <i class="mdi mdi-check-circle-outline"></i>
                      </span>
                    </div>
                  </div>
                  <p id="nomWarning"></p>
                </div>
                <div class="form-group">
                  <div class="input-group">
                    <input id = "prenom" type="text" class="form-control" placeholder="Prénom" pattern="[A-Za-z]([A-Za-z]|[0-9])*.{2,}" required>
                    <div class="input-group-append">
                      <span class="input-group-text"  id="prenomValide">
                        <i class="mdi mdi-check-circle-outline"></i>
                      </span>
                    </div>
                  </div>
                  <p id="prenomWarning"></p>
                </div>
                <div class="form-group">
                  <div class="input-group">
                    <input id="email" type="email" class="form-control" placeholder="Email" required>
                    <div class="input-group-append">
                      <span class="input-group-text" id="emailValide">
                        <i class="mdi mdi-check-circle-outline"></i>
                      </span>
                    </div>
                  </div>
                  <p id="emailWarning"></p>
                </div>
                <div class="form-group">
                  <div class="input-group">
                    <input id = "telf" type="tel" class="form-control" placeholder="Téléphone" pattern="[0-9]{10}" required>
                    <div class="input-group-append">
                      <span class="input-group-text"  id="telfValide">
                        <i class="mdi mdi-check-circle-outline"></i>
                      </span>
                    </div>
                  </div>
                  <p id="telfWarning"></p>
                </div>
                <div class="form-group">
                  <div class="input-group">
                    <input id = "adresse" type="text" class="form-control" placeholder="Adresse" required>
                    <div class="input-group-append">
                      <span class="input-group-text"  id="adresseValide">
                        <i class="mdi mdi-check-circle-outline"></i>
                      </span>
                    </div>
                  </div>
                  <p id="adresseeWarning"></p>
                </div>
                <div class="form-group">
                  <div class="input-group">
                    <input id="password" type="password" class="form-control" placeholder="Mot de passe" pattern=".{6,}" required>
                    <div class="input-group-append">
                      <span class="input-group-text" id="passwordValide">
                        <i class="mdi mdi-check-circle-outline"></i>
                      </span>
                    </div>
                  </div>
                  <p id="passwordWarning"></p>
                </div>
                <div class="form-group">
                  <div class="input-group">
                    <input id="passwordConf" type="password" class="form-control" placeholder="Confirmer mot de passe" pattern=".{6,}" required>
                    <div class="input-group-append">
                      <span class="input-group-text" id="passwordConfValide">
                        <i class="mdi mdi-check-circle-outline"></i>
                      </span>
                    </div>
                  </div>
                  <p id="passwordConfWarning"></p>
                </div>
                <div class="form-group">
                  <button id="register" class="btn btn-primary submit-btn btn-block">Inscrit</button>
                </div>
                <div id="login" class="text-block text-center my-3">
                  <span class="text-small font-weight-semibold">Vous avez déjà un compte?</span>
                  <a href="/login" class="text-black text-small">Connecter</a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- content-wrapper ends -->
    </div>
    <!-- page-body-wrapper ends -->
  </div>
  <!-- container-scroller -->
  <!-- plugins:js -->
  <script src="../../vendors/js/vendor.bundle.base.js"></script>
  <script src="../../vendors/js/vendor.bundle.addons.js"></script>
  <!-- endinject -->
  <!-- inject:js -->
  <script src="../../js/off-canvas.js"></script>
  <script src="../../js/misc.js"></script>
  <!-- endinject -->
   <!--debut Verifier les renseignements--> 
  <script src="js/authentification/register.js"></script>
  <!--end Verifier les renseignements--> 
</body>

</html>